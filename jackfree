#include<stdio.h>
#include<stdlib.h>

int main()
{
    int a,b,c,d,cur,sum=0;
    int arr[100];
    scanf("%d %d %d \n %d",&a,&b,&c,&d);
    for(int i=0;i<a;i++){
        arr[i]=1;
    }
    for(int i=a;i<a+b;i++){
        arr[i]=2;
    }
    for(int i=a+b;i<a+b+c;i++){
        arr[i]=5;
    }
    if(d==0){
	}
    for(int i=d;i<a+b+c;i++){
        sum=sum+arr[i];
    }
    printf("%d",sum);

}
